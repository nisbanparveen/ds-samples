
public class DynamicArrayStack extends SimpleArrayStack {

	public void push(int data) {
		if (isStackFull())
			doubleStak();
		super.array[++super.top] = data;
		System.out.println(data+" Pushed into the stack");
	}

	private void doubleStak() {
		int[] newArray= new int[super.capacity*2];
		super.capacity=capacity*2;
		super.array=newArray;
	}
}
