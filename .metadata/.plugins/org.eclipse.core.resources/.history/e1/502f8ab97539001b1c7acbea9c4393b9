package LinkedList;

import LinkedList.LinkedListCopy.Node;

public class Deletion {
	
	public LinkedListCopy deleteFirst(LinkedListCopy list) {
		Node tempNode= list.head;
		
		if(tempNode != null) {
			list.head= tempNode.next;
		}
		return list;
	}
	
	public static void main(String[] args) {
		LinkedListCopy list= new LinkedListCopy();
		list.insert(list, 1);
		list.insert(list, 2);
		list.insert(list, 3);
		
		list.printList(list);
		Deletion delete= new Deletion();
		delete.deleteFirst(list);
		delete.deleteFirst(list);

		list.printList(list);
	}

}
