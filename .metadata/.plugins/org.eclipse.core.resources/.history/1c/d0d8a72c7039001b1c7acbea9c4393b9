import LinkedList.FindIntersectingNode;
import LinkedList.LinkedList;
import LinkedList.LinkedList.Node;
import LinkedList.LinkedListNode;

public class Main {

	public static void main(String[] args) {

		LinkedListNode list1 = new LinkedListNode();

		list1.insert(list1, 'a');
		list1.insert(list1, 'b');
		list1.insert(list1, 'c');
		list1.insert(list1, 'd');
		list1.insert(list1, 'e');
		list1.insert(list1, 'f');
		list1.printList(list1);

		LinkedListNode list2 = new LinkedListNode();
		list2.insert(list2, 'x');
		list2.insert(list2, 'y');
		list2.insert(list2, 'c');
		list2.insert(list2, 'd');
		list2.insert(list2, 'e');
		list2.insert(list2, 'f');

		list2.printList(list2);

		System.out.println("Insertion: " + FindIntersectingNode.FindNode(list1, list2));

		
		LinkedListNode list3 = new LinkedListNode();

		list3.insert(list3, 3);
		list3.insert(list3, 6);
		list3.insert(list3, 9);
		list3.insert(list3, 15);
		list3.insert(list3, 30);
		list3.insert(list3, 40);

		LinkedListNode list4 = new LinkedListNode();
		list4.insert(list4, 10);
		list4.insert(list4, 15);
		list4.insert(list4, 30);
		
	}

}
